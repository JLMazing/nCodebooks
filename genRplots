png(filename="SVM_0.png")
d = read.table("statData_SVM-0", header=TRUE)
par(mar = c(5,5,2,5))
with(d, plot(itIdx, Objective, type="l", col="red3", main="SVM 0", ylab="Prim. Objective"))
par(new = T)
with(d, plot(itIdx, X.HyperPlane., type="l", col="blue3", axes=F, xlab=NA, ylab=NA, cex=1.2))
axis(side=4)
mtext(side=4, line=3, "HyperPlane Length")
legend("topleft", legend=c("Objective", "Hyperplane"), lty=c(1,1), col=c("red3", "blue3"))
dev.off()


